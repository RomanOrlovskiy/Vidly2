@model Vidly2.Models.Movie
@{
    ViewBag.Title = "Details";
    Layout = "~/Views/Shared/_Layout.cshtml";
}

<h2>@Model.Name</h2>
<ul>
    <li> Genre: @Model.Genre.Name</li>
    <li> Release date: @Model.ReleaseDate.ToShortDateString()</li>
    <li> Release date: @Model.DateAdded.ToShortDateString()</li>
    <li> Number in stock: @Model.NumberInStock</li>
</ul>

